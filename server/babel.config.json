{
  "presets": ["@babel/react"],
  "babelrcRoots": ["."],
  "plugins": [
    ["@babel/plugin-proposal-class-properties"],
    ["@babel/plugin-syntax-object-rest-spread"],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./src/components",
          "article": "./src/modules/article",
          "category": "./src/modules/category",
          "contact": "./src/modules/contact",
          "display": "./src/modules/display",
          "lib": "./src/lib",
          "main": "./src/modules/main",
          "route": "./src/modules/route",
          "routes": "./src/routes",
          "styles": "./src/styles",
          "utils": "./src/utils"
        }
      }
    ]
  ]
}
